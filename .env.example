# Environment Configuration for Ultimate Trading System
# Copy this file to .env and fill in your actual values

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_USER_ID=your_telegram_user_id_here
TELEGRAM_CHANNEL_ID=@your_channel_name_here

# Pocket Option Configuration
POCKET_OPTION_SSID=your_pocket_option_session_id_here
POCKET_OPTION_BASE_URL=https://pocketoption.com
POCKET_OPTION_WS_URL=wss://pocketoption.com/ws

# Database Configuration (optional overrides)
# DATABASE_URL=sqlite:///workspace/data/trading.db
# REDIS_URL=redis://localhost:6379

# Logging Configuration (optional)
# LOG_LEVEL=INFO
# LOG_FILE=/workspace/logs/trading_system.log

# Model Configuration (optional)
# MODEL_CACHE_DIR=/workspace/models/cache
# FEATURE_CACHE_DIR=/workspace/features/cache

# Security Configuration (optional)
# ENCRYPTION_KEY=your_32_character_encryption_key_here
# API_RATE_LIMIT=1000